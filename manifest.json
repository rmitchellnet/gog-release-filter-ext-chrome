{
    "name": "GOG Release Date Filter",
    "version": "0.1",
    "content_scripts": [{
      "matches": ["http://www.gog.com/*", "https://www.gog.com/*"],
      "js": ["main.js"]
    }],
    "web_accessible_resources": [
      "content.styles.css",
      "icon-128.png",
      "icon-34.png"
    ],
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }